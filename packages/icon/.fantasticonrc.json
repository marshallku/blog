{
  "inputDir": "./src/icons",
  "outputDir": "./dist",
  "fontTypes": ["woff", "woff2"],
  "assetTypes": ["css", "json", "ts"],
  "name": "icons",
  "prefix": "icon",
  "selector": ".icon",
  "fontsUrl": "./",
  "formatOptions": {
    "json": {
      "indent": 2
    }
  },
  "templates": {
    "css": "./src/templates/css.hbs"
  },
  "pathOptions": {
    "json": "./dist/icons.json",
    "css": "./dist/icons.css",
    "ts": "./dist/icons.ts"
  }
}
